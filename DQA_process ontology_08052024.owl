    <owl:ObjectProperty rdf:about="http://local.tamu.edu/djlee/ns/dqa#creates">
        <rdfs:domain rdf:resource="https://www.w3.org/TR/prov-o/#Agent"/>
        <rdfs:range rdf:resource="http://local.tamu.edu/djlee/ns/dqa#InformationProduct"/>
        <skos:definition>Indicate A is creator of B
(source: Schema.org, DDI)</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://local.tamu.edu/djlee/ns/dqa#hasQualityActiveModel -->

    <owl:ObjectProperty rdf:about="http://local.tamu.edu/djlee/ns/dqa#hasQualityActiveModel">
        <rdfs:domain rdf:resource="http://local.tamu.edu/djlee/ns/dqa#InformationProduct"/>
        <rdfs:range rdf:resource="http://local.tamu.edu/djlee/ns/dqa#Quality_Model_Active"/>
        <rdfs:comment>Indicates that a Quality Model is applicable to a specific InformationProduct</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://local.tamu.edu/djlee/ns/dqa#hasQualityProblem -->

    <owl:ObjectProperty rdf:about="http://local.tamu.edu/djlee/ns/dqa#hasQualityProblem">
        <rdfs:domain rdf:resource="http://local.tamu.edu/djlee/ns/dqa#InformationProduct"/>
        <rdfs:range rdf:resource="http://local.tamu.edu/djlee/ns/dqa#Quality_Problem"/>
        <rdfs:comment>Connects inforamtion Product to its Quality Problems.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://local.tamu.edu/djlee/ns/dqa#hasQualityRequiredModel -->

    <owl:ObjectProperty rdf:about="http://local.tamu.edu/djlee/ns/dqa#hasQualityRequiredModel">
        <rdfs:domain rdf:resource="http://local.tamu.edu/djlee/ns/dqa#InformationProduct"/>
        <rdfs:range rdf:resource="http://local.tamu.edu/djlee/ns/dqa#Quality_Model_Required"/>
