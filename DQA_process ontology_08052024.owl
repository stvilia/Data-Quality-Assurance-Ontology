    <!-- http://local.tamu.edu/djlee/ns/dqa#collects -->

    <owl:ObjectProperty rdf:about="http://local.tamu.edu/djlee/ns/dqa#collects">
        <rdfs:domain rdf:resource="https://www.w3.org/TR/prov-o/#Agent"/>
